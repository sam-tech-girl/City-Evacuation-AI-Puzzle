{
  "version": 2,
  "comment": "City Evacuation AI â€” Vercel deployment config for React frontend",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://your-backend.onrender.com"
  }
}
